{
	"db_name": "Mixtape",
	"db_file": "merged-Mixtape/Mixtape.db",
	"version": 3,
	"maps": {
		".+/data/users/.+/content.json": {
			"to_json_table": ["cert_user_id"],
			"file_name": "data.json"
		},
		".+/data/users/.+/data.json": {
			"to_table": [
				"song"
			],
			"to_json_table": ["hub"]
		},
		"data.json": {
			"to_table": [ "public_hub" ]
		}
	},
	"tables": {
		"json": {
			"cols": [
				["json_id", "INTEGER PRIMARY KEY AUTOINCREMENT"],
				["site", "TEXT"],
				["directory", "TEXT"],
				["file_name", "TEXT"],
				["cert_user_id", "TEXT"],
				["hub", "TEXT"]
			],
			"indexes": ["CREATE UNIQUE INDEX path ON json(directory, site, file_name)"],
			"schema_changed": 1
		},
		"song": {
			"cols": [
				["song_id", "INTEGER"],
				["title", "TEXT"],
				["artist", "TEXT"],
				["file_name", "TEXT"],
				["date_added", "INTEGER"],
				["json_id", "INTEGER REFERENCES json (json_id)"]
			],
			"indexes": ["CREATE UNIQUE INDEX song_key ON song(json_id, song_id)", "CREATE INDEX song_id ON song(song_id)", "CREATE INDEX added ON song(date_added)"],
			"schema_changed": 1
		}
	}
}
